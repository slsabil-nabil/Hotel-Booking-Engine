{
    "name": "slsabil/laravel-hotel-booking",
    "description": "Laravel Hotel Booking Engine",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Slsabil\\LaravelHotelBooking\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Slsabil\\LaravelHotelBooking\\Database\\Factories\\": "database/factories/",
            "Slsabil\\LaravelHotelBooking\\Tests\\": "tests"
        }
    },
    "require-dev": {
        "pestphp/pest": "^2.30",
        "orchestra/testbench": "^8.0",
        "pestphp/pest-plugin-laravel": "^2.4",
        "fakerphp/faker": "^1.23",
        "phpunit/phpunit": "^9.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Slsabil\\LaravelHotelBooking\\HotelBookingServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}