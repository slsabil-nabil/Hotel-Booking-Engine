{
    "name": "slsabil/laravel-hotel-booking",
    "description": "Laravel Hotel Booking Engine",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Slsabil\\LaravelHotelBooking\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Slsabil\\LaravelHotelBooking\\HotelBookingServiceProvider"
            ]
        }
    },
    "require-dev": {
        "pestphp/pest": "^2.30",
        "orchestra/testbench": "^8.0",
        "pestphp/pest-plugin-laravel": "^2.4"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
